<view class=''>
  <view class="music {{isPlay? 'rolling':''}}" bindtap="onPlay">
    <text class='iconfont iconyinle'></text>
  </view>
  <swiper autoplay duration="1000" interval="3000" circular indicator-dots indicator-color='rgba(255,255,255,0.6)' indicator-active-color='#fff'>
    <swiper-item wx:for='{{imgs}}' wx:key='item.id'>
      <image src="{{item.url}}"></image>
    </swiper-item>
  </swiper>
  <view class='item-wrap'>
    <view class='left'>
      <text class='iconfont iconuser1 text'></text>
      <text class='item-title'>个人信息</text>
    </view>
    <view class='right'>
      <view class='title-wrap border-bottom'>
        <text class='iconfont iconmale'></text>
        <text class='title'> 大树</text>
      </view>
      <view class='title-wrap border-bottom'>
        <text class='iconfont icondangwugongkai'></text>
        <text class='title'> 党员</text>
      </view>
      <view class='title-wrap border-bottom' bindtap='checkLocation'>
        <text class='iconfont iconweizhi'></text>
        <text class='title'> 桂林.七星万达</text>
        <text class='location'>(查看位置)</text>
      </view>
      <view class='title-wrap border-bottom'>
        <text class='iconfont iconliwu'></text>
        <text class='title'> 08.22</text>
      </view>
      <view class='title-wrap border-bottom'>
        <text class='iconfont iconxuewei'></text>
        <text class='title'> 工科硕士</text>
      </view>
      <view class='title-wrap border-bottom' bindtap='makeCall'>
        <text class='iconfont icontel-fill'></text>
        <text class='title'> 13707735481</text>
        <text class='cell'>(点击拨打)</text>
      </view>
      <view class='title-wrap border-bottom'>
        <text class='iconfont iconmail'></text>
        <text class='title'> 1942194789@qq.com</text>
      </view>
    </view>
  </view>

  <view class='item-wrap'>
    <view class='left'>
      <text class='iconfont iconshujijiaocai text'></text>
      <text class='item-title'>{{info[0].title}}</text>
    </view>
    <view class='right'>
      <view class='title-wrap' wx:for='{{info[0].cont}}' wx:key='item'>
        <view class='skill'>
          <view style='width:{{item.per}}%'>
            <text class='title'>{{item.skill}}</text>
          </view>
        </view>
      </view>
    </view>
  </view>

  <view class='item-wrap'>
    <view class='left'>
      <text class='iconfont icongongwenbao text'></text>
      <text class='item-title'>胜任岗位</text>
    </view>
    <view class='right'>
      <view class='title-wrap border-bottom'>
        <text class='title'>WEB前端</text>-
        <text class='sub-title'>Web R&D</text>
      </view>
      <view class='title-wrap border-bottom'>
        <text class='title'>微信小程序</text>-
        <text class='sub-title'>Mini Programme</text>
      </view>
      <view class='title-wrap border-bottom'>
        <text class='title'>培训讲师</text>-
        <text class='sub-title'>Training</text>
      </view>
      <view class='title-wrap border-bottom'>
        <text class='title'>策划/架构</text>-
        <text class='sub-title'>System Architect</text>
      </view>
      <view class='title-wrap border-bottom'>
        <text class='title'>行政人事</text>-
        <text class='sub-title'>Human Resource</text>
      </view>
    </view>
  </view>

  <view class='item-wrap'>
    <view class='left'>
      <text class='iconfont icongongsiquanbushengchandingdanguanli text'></text>
      <text class='item-title'>工作经历</text>
    </view>
    <view class='right'>
      <view class='title-wrap border-bottom'>
        <text class='title'>广州军区通信训练大队</text>-
        <text class='sub-title'>无线室</text>
      </view>
      <view class='title-wrap border-bottom'>
        <text class='title'>广州军区通信训练大队</text>-
        <text class='sub-title'>电源室</text>
      </view>
      <view class='title-wrap border-bottom'>
        <text class='title'>广州军区综合训练基地</text>-
        <text class='sub-title'>通信室</text>
      </view>
      <view class='title-wrap border-bottom'>
        <text class='title'>陆军特种作战学院</text>-
        <text class='sub-title'>侦察系</text>
      </view>
      <!-- <view class='title border-bottom' wx:for='{{info[2].cont}}' wx:key='item'>{{item}}</view> -->
    </view>
  </view>



  <view class='item-wrap'>
    <view class='left'>
      <text class='iconfont iconFavorite text'></text>
      <text class='item-title'>兴趣爱好</text>
    </view>
    <view class='right'>
      <view class='title-wrap border-bottom'>
        <text class='title'>电影</text>-
        <text class='sub-title'>动漫 科幻 记录</text>
      </view>
      <view class='title-wrap border-bottom'>
        <text class='title'>美剧</text>-
        <text class='sub-title'>科幻</text>
      </view>
      <view class='title-wrap border-bottom'>
        <text class='title'>运动</text>-
        <text class='sub-title'>跑步 健身 游泳</text>
      </view>
      <view class='title-wrap border-bottom'>
        <text class='title'>美食</text>-
        <text class='sub-title'>饺子 牛肉面 火锅</text>
      </view>
      <view class='title-wrap border-bottom'>
        <text class='title'>绿植</text>-
        <text class='sub-title'>多肉</text>
      </view>
      <view class='title-wrap border-bottom'>
        <text class='title'>发呆</text>-
        <text class='sub-title'>... ...</text>
      </view>
    </view>
  </view>
  <button class='btn' bindtap="toMsg">感兴趣就给我留言吧</button>
</view>